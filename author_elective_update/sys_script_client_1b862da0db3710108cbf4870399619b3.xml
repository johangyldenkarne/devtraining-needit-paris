        <messages/>
        <name>Request type change</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
		alert('onChange was fired');
		return;
   }
	
	
	
	if(g_form.getValue('u_request_type') == 'hr'){
		g_form.setValue('u_what_needed', 'hr1');
		return;
	}
	
	if(g_form.getValue('u_request_type') == "legal"){
		g_form.setValue('u_what_needed', 'legal1');
		return;
	}
	
	if(g_form.getValue('u_request_type') == 'facilities'){
		g_form.setValue('u_what_needed', 'facilities1');
		return;
	}
	
	
	if(g_form.getValue('u_request_type') == 'your_choice'){
		g_form.setValue('u_what_needed', 'your_choice1');
		return;
	}
   
}]]></script>
